{
    "meta": {
        "name": "User API Documentation",
        "type": "http",
        "seq": 1
    },
    "http": {
        "method": "get",
        "url": "https://api.example.com/users"
    },
    "examples": [
        {
            "meta": {
                "name": "Get User by ID",
                "type": "http",
                "seq": 1
            },
            "http": {
                "method": "get",
                "url": "https://api.example.com/users/123"
            },
            "headers": [
                {
                    "name": "authorization",
                    "value": "Bearer token123",
                    "enabled": true
                }
            ],
            "response": {
                "status": {
                    "code": "200"
                },
                "body": "{\n  \"id\": 123,\n  \"name\": \"John Doe\",\n  \"email\": \"john@example.com\"\n}"
            }
        },
        {
            "meta": {
                "name": "Create New User",
                "type": "http",
                "seq": 1
            },
            "http": {
                "method": "post",
                "url": "https://api.example.com/users"
            },
            "headers": [
                {
                    "name": "content-type",
                    "value": "application/json",
                    "enabled": true
                },
                {
                    "name": "authorization",
                    "value": "Bearer token123",
                    "enabled": true
                }
            ],
            "body": {
                "json": "{\n  \"name\": \"New User\",\n  \"email\": \"newuser@example.com\"\n}"
            },
            "response": {
                "status": {
                    "code": "201"
                },
                "body": "{\n  \"id\": 456,\n  \"name\": \"New User\",\n  \"email\": \"newuser@example.com\",\n  \"created_at\": \"2023-01-15T10:30:00Z\"\n}"
            }
        }
    ]
}