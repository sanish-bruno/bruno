          "json": "{\n  \"name\": \"New User\",\n  \"email\": \"newuser@example.com\"\n}"
        }
      },
      "response": {
        "status": "201",
        "statusText": "OK",
        "body": "\"id\": 456,\n\"name\": \"New User\",\n\"email\": \"newuser@example.com\",\n\"created_at\": \"2023-01-15T10:30:00Z\""
      }
